            <col class="event-outcome">
            <col class="event-outcome">
            <col class="event-outcome">
        {% endif %}
    </colgroup>
    <tbody>
    {# hiding league details row #}
    {% if not small %}
    <tr class="league-data active">
        <th colspan="{{ colspan }}">
            <div class="tools">
                <span>
                  <a class="fav active" href="#"><span class="glyphicon glyphicon-star"></span></a>
                </span>
            </div>
            <a class="dropdown-toggle" href="#"><i class="icon-caret-down"></i></a>
            <div class="title">
                <span class="country"><i class="country-flag fr"></i>France :</span>
                <a href="#">Ligue 2 </a>
            </div>
        </th>
    </tr>
    {% endif %}
