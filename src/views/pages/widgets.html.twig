     {% include 'blocks/sports/top_competitions_results.html.twig' %}

        <h3>Match listing (general)</h3>
        {{ include('blocks/sports/listings/match_listing_general.html.twig', {in_panel:'true', noHeader:'true'}) }}
        {{ include('blocks/sports/listings/match_listing_general_odds.html.twig', {in_panel:'true', noHeader:'true'}) }}
        {{ include('blocks/sports/listings/match_listing_general_predictions.html.twig', {in_panel:'true', noHeader:'true'}) }}

