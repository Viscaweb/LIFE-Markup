        <div class="row"><div class="col-xs-12">{% include 'blocks/sports/competition/competition_standing.html.twig' %}</div></div>
        <div class="row"><div class="col-xs-12">{% include 'blocks/sports/competition/competition_standing_groups.html.twig' %}</div></div>
